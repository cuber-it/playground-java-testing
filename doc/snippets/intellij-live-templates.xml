  <!-- Builder Pattern -->
  <template name="builder" value="public class $NAME$Builder {&#10;&#10;    private Long id = 1L;&#10;    private String $FIELD1$ = &quot;default&quot;;&#10;&#10;    private $NAME$Builder() {}&#10;&#10;    public static $NAME$Builder a$NAME$() {&#10;        return new $NAME$Builder();&#10;    }&#10;&#10;    public $NAME$Builder withId(Long id) {&#10;        this.id = id;&#10;        return this;&#10;    }&#10;&#10;    public $NAME$Builder with$FIELD1_CAP$(String $FIELD1$) {&#10;        this.$FIELD1$ = $FIELD1$;&#10;        return this;&#10;    }&#10;&#10;    public $NAME$ build() {&#10;        $NAME$ obj = new $NAME$();&#10;        obj.setId(id);&#10;        obj.set$FIELD1_CAP$($FIELD1$);&#10;        return obj;&#10;    }&#10;}" description="Test Data Builder" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;Todo&quot;" alwaysStopAt="true"/>
    <variable name="FIELD1" expression="" defaultValue="&quot;title&quot;" alwaysStopAt="true"/>
    <variable name="FIELD1_CAP" expression="" defaultValue="&quot;Title&quot;" alwaysStopAt="true"/>
    <context>
